cc_prebuilt_library_shared {
    name: "libmtklimiter",
    owner: "mtk",

    arch: {
        arm: {
            srcs: ["libmtklimiter.so"],
        },

        arm64: {
            srcs: ["lib64mtklimiter.so"],
        },
    },
    compile_multilib: "both",
}

cc_prebuilt_library_shared {
    name: "libmtklimiter_vendor",
    proprietary: true,
    owner: "mtk",

    arch: {
        arm: {
            srcs: ["libmtklimiter_vendor.so"],
        },

        arm64: {
            srcs: ["lib64mtklimiter_vendor.so"],
        },
    },
    compile_multilib: "both",
    shared_libs: ["libmtk_drvb"],
}
